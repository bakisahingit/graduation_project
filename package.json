		"express-rate-limit": "^8.2.1",
		"jsonwebtoken": "^9.0.2",
		"node-fetch": "^2.6.7",
		"openai": "^4.10.0",
		"redis": "^4.6.14",
		"uuid": "^9.0.0",
		"ws": "^8.17.1"
	},
	"devDependencies": {
		"concurrently": "^9.2.1",
		"nodemon": "^3.1.10"
	}
